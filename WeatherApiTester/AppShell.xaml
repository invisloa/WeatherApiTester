<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WeatherApiTester.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WeatherApiTester"
    >

    <FlyoutItem Title="SomeTitle">
        <ShellContent Icon="appicon.svg"
            Title="Dashboart"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="Dashboart" />

        <ShellContent Icon="appicon.svg"
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
    </FlyoutItem>
</Shell>
